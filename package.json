{
  "name": "css-custom-property-extractor",
  "version": "0.1.6",
  "description": "Extract css custom properties as constant values that can be used in JS.",
  "main": "dist/cli.js",
  "module": "dist/cli.mjs",
  "exports": {
    ".": {
      "require": "./dist/cli.js",
      "import": "./dist/cli.mjs"
    }
  },
  "bin": {
    "ccpe": "dist/cli.js"
  },
  "scripts": {
    "build": "turbo run tsup",
    "tsup": "tsup src/cli.ts --format esm,cjs --clean",
    "tsx": "tsx ./src/cli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeguild/css-custom-property-extractor.git"
  },
  "keywords": [
    "typescript",
    "extract",
    "custom-property",
    "css",
    "scss",
    "less"
  ],
  "author": "j1ngzoue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activeguild/css-custom-property-extractor/issues"
  },
  "homepage": "https://github.com/activeguild/css-custom-property-extractor#readme",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/less": "^3.0.3",
    "@types/node": "^18.15.13",
    "@types/sass": "^1.43.1",
    "css-custom-property-extractor": "^0.1.6",
    "esbuild": "^0.17.17",
    "less": "^4.1.3",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "commander": "^10.0.1",
    "glob": "^10.2.1",
    "vite": "^4.3.1"
  },
  "peerDependencies": {
    "less": "*",
    "sass": "*"
  }
}
