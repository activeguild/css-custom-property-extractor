{
  "name": "css-custom-property-extractor",
  "version": "1.0.0",
  "description": "Extract css custom properties as constant values that can be used in JS.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/cli.js",
      "import": "./dist/cli.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/cli.ts --format esm,cjs --clean",
    "extract": "tsx ./src/main.ts",
    "tsx": "tsx ./src/cli.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeguild/css-custom-property-extractor.git"
  },
  "keywords": [
    "typescript",
    "css",
    "extract",
    "custom-property"
  ],
  "author": "j1ngzoue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activeguild/css-custom-property-extractor/issues"
  },
  "homepage": "https://github.com/activeguild/css-custom-property-extractor#readme",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.17",
    "esbuild": "^0.16.9",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "glob": "^8.0.3"
  },
  "peerDependencies": {
    "sass": "*"
  }
}
