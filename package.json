{
  "name": "css-custom-property-extractor",
  "version": "0.1.2",
  "description": "Extract css custom properties as constant values that can be used in JS.",
  "main": "dist/cli.js",
  "module": "dist/cli.mjs",
  "exports": {
    ".": {
      "require": "./dist/cli.js",
      "import": "./dist/cli.mjs"
    }
  },
  "bin": {
    "ccpe": "dist/cli.js"
  },
  "scripts": {
    "build": "turbo run tsup",
    "tsup": "tsup src/cli.ts --format esm,cjs --clean",
    "tsx": "tsx ./src/cli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeguild/css-custom-property-extractor.git"
  },
  "keywords": [
    "typescript",
    "extract",
    "custom-property",
    "css",
    "scss",
    "less"
  ],
  "author": "j1ngzoue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activeguild/css-custom-property-extractor/issues"
  },
  "homepage": "https://github.com/activeguild/css-custom-property-extractor#readme",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.17",
    "css-custom-property-extractor": "^0.1.0",
    "esbuild": "^0.16.9",
    "less": "^4.1.3",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "glob": "^8.0.3"
  },
  "peerDependencies": {
    "less": "*",
    "sass": "*"
  }
}
